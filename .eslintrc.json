{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "double"],
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "no-console": "off"
  }
}
